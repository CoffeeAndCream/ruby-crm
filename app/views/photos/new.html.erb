<div class="container">
  <div class="col-md-6 col-md-offset-3 info-card">
    <h1>New photos</h1>

    <%= bootstrap_form_for [@lead, @photo] do |f| %>
        <%= f.file_field :image, required: true, :multiple => true %>
        <%= f.text_field :title, class: 'form-control input-sm', style: 'width: 100%', label: "Caption" %>
      <%= hidden_field_tag :lead_id => @lead.id %>
      <%= f.submit 'Submit', class: 'btn btn-primary btn-md' %>
    <% end %>
  </div>
</div>
