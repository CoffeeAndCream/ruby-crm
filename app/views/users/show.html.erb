<div class="container">
  <div class="row">
      <div class="col-md-1">
        <i class="material-icons" style="font-size: 80px;padding-top: 10px; float: left">account_box</i>
      </div>
      <div class="col-md-8">
        <h3><%= @user.first_name %> <%= @user.last_name %></h3>
        <h5><%= @user.email %></h5>
      </div>
    </div>
    <div class="row">
      <div id="closing-stats">
        Closing Percentage: <%= ((Lead.where(user_id: @user.id).where(customer: true).count.to_f / Lead.where(user_id: @user.id).count.to_f)*100).round(2) %>%
      </div>
    </div>
    <div class="col-md-2" style="float: right;padding-top: 0px">
      <%= render 'links' %>
    </div>
  </div>
</div>
<br>
<% render 'daily_schedule' %>
<%= render 'user_calendar' %>
<%= render 'leads' %>
