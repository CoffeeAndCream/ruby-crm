  <body class="login-page login-background">
  <h1 style="color:#4f4a6b;font-weight:bold;"><img src="/images/favicon.ico" height="75px"></img> Legacy Exteriors CRM </h1>
  <div class="login-container  info-card col-md-4 col-md-offset-4">
    <%= render 'layouts/flash' %>
    <%= yield %>
  </div>
  <div class="col-md-1">
